	<Project>
		<Option title="DeSmuME Cocoa" />
		<Option platforms="Mac;" />
		<Option pch_mode="2" />
		<Option default_target="Cocoa X86" />
		<Option compiler="gcc" />
		<Option show_notes="0">
			<notes>
